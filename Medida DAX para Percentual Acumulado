## MEDIDA DAX PARA C√ÅLCULO DE VALOR PERCENTUAL ACUMULADO

% Faturamento Acumulado = 

VAR FATURAMENTO_TOTAL = CALCULATE( [Faturamento], ALL( d_Calendario ) )
VAR FATURAMENTO_ACUMULADO = 
CALCULATE(
    [Faturamento],
    FILTER(
        ALL(d_Calendario),
        d_Calendario[Date] <= MIN( d_Calendario[Date] )
    )
)
VAR PERCENTUAL_FATURAMENTO_ACUMULADO = DIVIDE( FATURAMENTO_ACUMULADO, FATURAMENTO_TOTAL )

RETURN
PERCENTUAL_FATURAMENTO_ACUMULADO
