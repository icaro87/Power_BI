## MEDIDA DAX PARA CÁLCULO DE MÉDIA MOVEL - 3 MESES

Media Movel Faturamento = 

VAR FAT_ULTIMOS_3_MESES = 
CALCULATE(
    [Faturamento],
    DATEADD( DATESINPERIOD( d_Calendario[Date], LASTDATE( d_Calendario[Date] ), -3, MONTH ), -1, MONTH /* DESCONSIDERANDO O MES ATUAL */ )
)

VAR MEDIA_MOVEL_3_MESES = DIVIDE( FAT_ULTIMOS_3_MESES, 3, 0 )

RETURN 
MEDIA_MOVEL_3_MESES
